{
    "name": "study-stack",
    "version": "1.1.2",
    "private": true,
    "scripts": {
        "dev": "scripts/shared/run.sh vite dev --host 0.0.0.0",
        "pscale:branch:create": "scripts/shared/run.sh scripts/pscale/create-branch.sh",
        "pscale:branch:delete": "scripts/shared/run.sh scripts/pscale/delete-branch.sh",
        "pscale:branch:delete-all-dev": "scripts/shared/run.sh scripts/pscale/delete-all-dev-branches.sh",
        "pscale:cred:delete-all": "scripts/shared/run.sh scripts/pscale/delete-all-creds.sh",
        "pscale:cred:delete-all-ci": "scripts/shared/run.sh scripts/pscale/delete-all-ci-creds.sh",
        "pscale:cred:create": "scripts/shared/run.sh scripts/pscale/create-cred.sh",
        "pscale:dr:create": "scripts/shared/run.sh scripts/pscale/create-deploy-request.sh",
        "pscale:dr:close": "scripts/shared/run.sh scripts/pscale/close-deploy-request.sh",
        "pscale:dr:apply": "scripts/shared/run.sh scripts/pscale/apply-deploy-request.sh",
        "postinstall": "prisma generate",
        "vercel:deploy:preview": "scripts/shared/run.sh scripts/vercel/deploy-preview.sh",
        "vercel:deploy:production": "scripts/shared/run.sh scripts/vercel/deploy-production.sh",
        "vercel:settings:pull": "scripts/shared/run.sh scripts/vercel/pull-settings.sh",
        "build": "vite build",
        "build:production": "scripts/shared/run.sh vite build --mode production",
        "preview": "scripts/shared/run.sh vite preview --host 0.0.0.0",
        "test": "playwright install && playwright test",
        "check": "scripts/shared/run.sh svelte-kit sync && scripts/shared/run.sh svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "scripts/shared/run.sh svelte-kit sync && scripts/shared/run.sh svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "scripts/shared/run.sh concurrently \"vite dev --port 3004 --host 0.0.0.0 --mode test\" \"vitest --run\" --kill-others -s first",
        "test:watch": "scripts/shared/run.sh concurrently \"vite dev --port 3004 --host 0.0.0.0 --mode test\" \"vitest\" --kill-others",
        "codegen": "scripts/shared/run.sh concurrently \"vite dev --port 3005 --host 0.0.0.0 --mode test\" \"playwright codegen\" --kill-others",
        "test:ui": "scripts/shared/run.sh concurrently \"vite dev --port 3005 --host 0.0.0.0 --mode test\" \"playwright test --ui\" --kill-others",
        "test:coverage": "scripts/shared/run.sh concurrently \"vite dev --port 3004 --host 0.0.0.0 --mode test\" \"vitest run --coverage\" --kill-others -s first",
        "test:results": "scripts/shared/run.sh pnpm exec playwright show-report e2e/test-results",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "lintfix": "prettier --plugin-search-dir . --write . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "prepare": "husky",
        "start:unit": "vite dev --port 3004 --host 0.0.0.0 --mode test"
    },
    "lint-staged": {
        "*.{js,ts,svelte}": "eslint",
        "*.{js,ts,svelte,css,scss,postcss,md,json}": [
            "prettier --write --plugin-search-dir=.",
            "prettier --check --plugin-search-dir=."
        ]
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "devDependencies": {
        "-": "^0.0.1",
        "@amplitude/analytics-browser": "^2.17.8",
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.28.0",
        "@faker-js/faker": "^9.8.0",
        "@floating-ui/dom": "^1.7.1",
