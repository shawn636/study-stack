{
    "name": "equipped",
    "version": "1.1.2",
    "private": true,
    "scripts": {
        "dev": "scripts/shared/run.sh vite dev",
        "pscale:branch:create": "scripts/shared/run.sh scripts/pscale/create-branch.sh",
        "pscale:branch:delete": "scripts/shared/run.sh scripts/pscale/delete-branch.sh",
        "pscale:branch:delete-all-dev": "scripts/shared/run.sh scripts/pscale/delete-all-dev-branches.sh",
        "pscale:cred:delete-all": "scripts/shared/run.sh scripts/pscale/delete-all-creds.sh",
        "pscale:cred:delete-all-ci": "scripts/shared/run.sh scripts/pscale/delete-all-ci-creds.sh",
        "pscale:cred:create": "scripts/shared/run.sh scripts/pscale/create-cred.sh",
        "pscale:dr:create": "scripts/shared/run.sh scripts/pscale/create-deploy-request.sh",
        "pscale:dr:close": "scripts/shared/run.sh scripts/pscale/close-deploy-request.sh",
        "pscale:dr:apply": "scripts/shared/run.sh scripts/pscale/apply-deploy-request.sh",
        "postinstall": "prisma generate",
        "vercel:deploy:preview": "scripts/shared/run.sh scripts/vercel/deploy-preview.sh",
        "vercel:deploy:production": "scripts/shared/run.sh scripts/vercel/deploy-production.sh",
        "vercel:settings:pull": "scripts/shared/run.sh scripts/vercel/pull-settings.sh",
        "build": "vite build",
        "build:production": "scripts/shared/run.sh vite build --mode production",
        "preview": "scripts/shared/run.sh vite preview",
        "test": "playwright install && playwright test",
        "check": "scripts/shared/run.sh svelte-kit sync && scripts/shared/run.sh svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "scripts/shared/run.sh svelte-kit sync && scripts/shared/run.sh svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "scripts/shared/run.sh concurrently \"vite dev --port 3004 --mode test\" \"vitest --run\" --kill-others -s first",
        "test:watch": "scripts/shared/run.sh concurrently \"vite dev --port 3004 --mode test\" \"vitest\" --kill-others",
        "codegen": "scripts/shared/run.sh concurrently \"vite dev --port 3005 --mode test\" \"playwright codegen\" --kill-others",
        "test:ui": "scripts/shared/run.sh concurrently \"vite dev --port 3005 --mode test\" \"playwright test --ui\" --kill-others",
        "test:coverage": "scripts/shared/run.sh concurrently \"vite dev --port 3004 --mode test\" \"vitest run --coverage\" --kill-others -s first",
        "test:results": "scripts/shared/run.sh pnpm exec playwright show-report e2e/test-results",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "lintfix": "prettier --plugin-search-dir . --write . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.{js,ts,svelte}": "eslint",
        "*.{js,ts,svelte,css,scss,postcss,md,json}": [
            "prettier --write --plugin-search-dir=.",
            "prettier --check --plugin-search-dir=."
        ]
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "devDependencies": {
        "@amplitude/analytics-browser": "^2.11.1",
        "@commitlint/cli": "^19.4.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@faker-js/faker": "^8.4.1",
        "@floating-ui/dom": "^1.6.10",
        "@fortawesome/free-brands-svg-icons": "^6.6.0",
        "@fortawesome/free-regular-svg-icons": "^6.6.0",
        "@fortawesome/free-solid-svg-icons": "^6.6.0",
        "@paralleldrive/cuid2": "^2.2.2",
        "@planetscale/database": "^1.19.0",
        "@playwright/test": "^1.46.1",
        "@prisma/adapter-planetscale": "^5.18.0",
        "@prisma/client": "^5.18.0",
        "@sveltejs/adapter-auto": "^3.2.4",
        "@sveltejs/adapter-vercel": "^5.4.3",
        "@sveltejs/kit": "^2.5.24",
        "@sveltejs/vite-plugin-svelte": "^3.1.2",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.14",
        "@testing-library/jest-dom": "^6.5.0",
        "@testing-library/svelte": "^5.2.1",
        "@types/bcryptjs": "^2.4.6",
        "@types/node": "^20.14.11",
        "@types/sanitize-html": "^2.13.0",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.16.1",
        "@typescript-eslint/parser": "^7.16.1",
        "@unovis/svelte": "^1.4.4",
        "@unovis/ts": "^1.4.4",
        "@vercel/analytics": "^1.3.1",
        "@vercel/node": "^3.2.9",
        "@vercel/speed-insights": "^1.0.12",
        "@vitest/coverage-v8": "^1.6.0",
        "autoprefixer": "^10.4.20",
        "bcryptjs": "^2.4.3",
        "bits-ui": "^0.21.13",
        "clsx": "^2.1.1",
        "concurrently": "^8.2.2",
        "dotenv": "^16.4.5",
        "dotenv-cli": "^7.4.2",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-filename-rules": "^1.3.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-perfectionist": "^2.11.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-svelte": "^2.43.0",
        "husky": "^9.1.5",
        "jsdom": "^24.1.0",
        "kysely": "^0.27.4",
        "kysely-planetscale": "^1.5.0",
        "lint-staged": "^15.2.9",
        "lottie-web": "^5.12.2",
        "mode-watcher": "^0.4.1",
        "module-alias": "^2.2.3",
        "postcss": "^8.4.41",
        "prettier": "^3.3.3",
        "prettier-eslint": "^16.3.0",
        "prettier-plugin-sh": "^0.14.0",
        "prettier-plugin-svelte": "^3.2.6",
        "prettier-plugin-tailwindcss": "^0.6.6",
        "prisma": "^5.18.0",
        "prisma-kysely": "^1.8.0",
        "sanitize-html": "^2.13.0",
        "shadcn-svelte": "^0.13.0",
        "svelte": "^4.2.19",
        "svelte-check": "^3.8.6",
        "svelte-fa": "^4.0.2",
        "svelte-forms-lib": "^2.0.1",
        "svelte-input-mask": "^1.1.0",
        "svelte-legos": "^0.2.5",
        "svelte-migrate": "^1.4.5",
        "svelte-sonner": "^0.3.27",
        "tailwind-merge": "^2.5.2",
        "tailwind-variants": "^0.2.1",
        "tailwindcss": "^3.4.10",
        "ts-interface-checker": "^1.0.2",
        "ts-node": "^10.9.2",
        "tslib": "^2.7.0",
        "tsx": "^4.18.0",
        "typescript": "^5.5.4",
        "uuid": "^9.0.1",
        "vercel": "^34.4.0",
        "vite": "^5.4.2",
        "vite-plugin-tailwind-purgecss": "^0.3.3",
        "vitest": "^1.6.0",
        "yup": "^1.4.0"
    },
    "type": "module",
    "packageManager": "pnpm@9.5.0+sha256.dbdf5961c32909fb030595a9daa1dae720162e658609a8f92f2fa99835510ca5",
    "optionalDependencies": {
        "@rollup/rollup-darwin-arm64": "^4.21.1"
    }
}
