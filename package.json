{
    "name": "equipped",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "vercel-dev": "vercel dev --token $VERCEL_TOKEN --yes",
        "build": "vite build",
        "build:production": "vite build --mode production",
        "preview": "vite preview",
        "test": "playwright install && playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "concurrently \"npm run vercel-dev\" \"vitest --run\" --kill-others -s first",
        "test:watch": "concurrently \"npm run vercel-dev\" \"vitest\" --kill-others",
        "codegen": "concurrently \"npm run vercel-dev\" \"playwright codegen\" --kill-others",
        "test:ui": "concurrently \"npm run vercel-dev\" \"playwright test --ui\" --kill-others",
        "test:coverage": "concurrently \"npm run vercel-dev\" \"vitest run --coverage\" --kill-others -s first",
        "test:results": "npx playwright show-report e2e/test-results",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "lintfix": "prettier --plugin-search-dir . --write . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "prepare": "husky install",
        "prisma-push": "./prisma/apply_scripts.sh"
    },
    "lint-staged": {
        "*.{js,ts,svelte}": "eslint",
        "*.{js,ts,svelte,css,scss,postcss,md,json}": [
            "prettier --write --plugin-search-dir=.",
            "prettier --check --plugin-search-dir=."
        ]
    },
    "prisma": {
        "seed": "node --experimental-specifier-resolution=node --loader ts-node/esm prisma/seed.ts"
    },
    "devDependencies": {
        "@amplitude/analytics-browser": "^2.3.8",
        "@commitlint/cli": "^18.4.4",
        "@commitlint/config-conventional": "^18.4.4",
        "@faker-js/faker": "^8.3.1",
        "@floating-ui/dom": "^1.5.3",
        "@fortawesome/free-brands-svg-icons": "^6.5.1",
        "@fortawesome/free-regular-svg-icons": "^6.5.1",
        "@fortawesome/free-solid-svg-icons": "^6.5.1",
        "@planetscale/database": "^1.13.0",
        "@playwright/test": "^1.40.1",
        "@prisma/client": "^5.7.1",
        "@skeletonlabs/skeleton": "^2.7.0",
        "@skeletonlabs/tw-plugin": "^0.3.1",
        "@sveltejs/adapter-auto": "^3.1.0",
        "@sveltejs/adapter-vercel": "^4.0.4",
        "@sveltejs/kit": "^2.0.6",
        "@sveltejs/vite-plugin-svelte": "^3.0.1",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "@testing-library/jest-dom": "^6.2.0",
        "@testing-library/svelte": "^4.0.5",
        "@types/bcryptjs": "^2.4.6",
        "@types/node": "^20.10.6",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "@vercel/node": "^3.0.15",
        "@vitest/coverage-v8": "^1.1.2",
        "autoprefixer": "^10.4.16",
        "bcryptjs": "^2.4.3",
        "concurrently": "^8.2.2",
        "dotenv": "^16.3.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.35.1",
        "husky": "^8.0.3",
        "jsdom": "^23.0.1",
        "lint-staged": "^15.2.0",
        "module-alias": "^2.2.3",
        "postcss": "^8.4.33",
        "prettier": "^3.1.1",
        "prettier-plugin-svelte": "^3.1.2",
        "prisma": "^5.8.1",
        "svelte": "^4.2.8",
        "svelte-check": "^3.6.2",
        "svelte-fa": "^3.0.4",
        "svelte-forms-lib": "^2.0.1",
        "svelte-input-mask": "^1.1.0",
        "svelte-migrate": "^1.3.6",
        "tailwindcss": "^3.4.0",
        "ts-interface-checker": "^1.0.2",
        "ts-node": "^10.9.2",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3",
        "uuid": "^9.0.1",
        "vercel": "^33.0.2",
        "vite": "^5.0.10",
        "vite-plugin-tailwind-purgecss": "^0.2.0",
        "vitest": "^1.1.2",
        "yup": "^1.3.3"
    },
    "type": "module"
}
