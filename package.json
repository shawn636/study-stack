{
	"name": "equipped",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"vercel-dev": "vercel dev --token $VERCEL_TOKEN --yes",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "concurrently \"npm run dev\" \"vitest --run\" --kill-others -s first",
		"test:watch": "concurrently \"npm run dev\" \"vitest\" --kill-others",
		"test:coverage": "concurrently \"npm run dev\" \"vitest run --coverage\" --kill-others -s first",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lintfix": "prettier --plugin-search-dir . --write . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": "eslint",
		"*.{js,ts,svelte,css,scss,postcss,md,json}": [
			"prettier --write --plugin-search-dir=.",
			"prettier --check --plugin-search-dir=."
		]
	},
	"prisma": {
		"seed": "node --experimental-specifier-resolution=node --loader ts-node/esm prisma/seed.ts"
	},
	"devDependencies": {
		"@amplitude/analytics-browser": "^1.10.4",
		"@commitlint/cli": "^17.6.5",
		"@commitlint/config-conventional": "^17.6.5",
		"@faker-js/faker": "^7.6.0",
		"@floating-ui/dom": "^1.2.7",
		"@fortawesome/free-brands-svg-icons": "^6.4.0",
		"@fortawesome/free-solid-svg-icons": "^6.4.0",
		"@planetscale/database": "^1.7.0",
		"@playwright/test": "^1.34.3",
		"@prisma/client": "^4.15.0",
		"@skeletonlabs/skeleton": "^1.6.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-vercel": "^2.4.3",
		"@sveltejs/kit": "^1.20.1",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@types/node": "^20.2.5",
		"@types/testing-library__jest-dom": "^5.14.6",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"@vercel/node": "^2.14.4",
		"@vitest/coverage-c8": "^0.29.8",
		"@vitest/coverage-istanbul": "^0.30.1",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.1.0",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.30.0",
		"husky": "^8.0.3",
		"jsdom": "^21.1.2",
		"lint-staged": "^13.2.2",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^4.15.0",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"svelte-fa": "^3.0.3",
		"tailwindcss": "^3.3.2",
		"ts-interface-checker": "^1.0.2",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.2",
		"typescript": "^5.1.3",
		"vercel": "^28.20.0",
		"vite": "^4.3.5",
		"vitest": "^0.30.1"
	},
	"type": "module"
}
