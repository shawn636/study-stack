{
	"name": "equipped",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"vercel-dev": "vercel dev --token $VERCEL_TOKEN --yes",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "concurrently \"npm run dev\" \"vitest --run\" --kill-others -s first",
		"test:watch": "concurrently \"npm run dev\" \"vitest\" --kill-others",
		"test:coverage": "concurrently \"npm run dev\" \"vitest run --coverage\" --kill-others -s first",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lintfix": "prettier --plugin-search-dir . --write . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": "eslint",
		"*.{js,ts,svelte,css,scss,postcss,md,json}": [
			"prettier --write --plugin-search-dir=.",
			"prettier --check --plugin-search-dir=."
		]
	},
	"prisma": {
		"seed": "node --experimental-specifier-resolution=node --loader ts-node/esm prisma/seed.ts"
	},
	"devDependencies": {
		"@amplitude/analytics-browser": "^1.10.3",
		"@commitlint/cli": "^17.6.3",
		"@commitlint/config-conventional": "^17.6.3",
		"@faker-js/faker": "^7.6.0",
		"@planetscale/database": "^1.7.0",
		"@playwright/test": "^1.33.0",
		"@prisma/client": "^4.14.0",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/adapter-vercel": "^2.4.3",
		"@sveltejs/kit": "^1.16.3",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@types/node": "^18.16.7",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@typescript-eslint/eslint-plugin": "^5.59.5",
		"@typescript-eslint/parser": "^5.59.5",
		"@vercel/node": "^2.14.2",
		"@vitest/coverage-c8": "^0.29.8",
		"@vitest/coverage-istanbul": "^0.30.1",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.0.1",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"jsdom": "^21.1.2",
		"lint-staged": "^13.2.2",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"prisma": "^4.14.0",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"svelte-heros-v2": "^0.4.2",
		"svelte-preprocess": "^5.0.3",
		"svelte-use-click-outside": "^1.0.0",
		"tailwindcss": "^3.3.2",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vercel": "^28.20.0",
		"vite": "^4.3.5",
		"vitest": "^0.31.0"
	},
	"type": "module"
}
