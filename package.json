{
    "name": "equipped",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "vercel-dev": "vercel dev --token $VERCEL_TOKEN --yes",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "concurrently \"npm run vercel-dev\" \"vitest --run\" --kill-others -s first",
        "test:watch": "concurrently \"npm run vercel-dev\" \"vitest\" --kill-others",
        "codegen": "concurrently \"npm run vercel-dev\" \"playwright codegen\" --kill-others",
        "test:ui": "concurrently \"npm run vercel-dev\" \"playwright test --ui\" --kill-others",
        "test:coverage": "concurrently \"npm run vercel-dev\" \"vitest run --coverage\" --kill-others -s first",
        "test:results": "npx playwright show-report e2e/test-results",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "lintfix": "prettier --plugin-search-dir . --write . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "prepare": "husky install",
        "prisma-push": "./prisma/apply_scripts.sh"
    },
    "lint-staged": {
        "*.{js,ts,svelte}": "eslint",
        "*.{js,ts,svelte,css,scss,postcss,md,json}": [
            "prettier --write --plugin-search-dir=.",
            "prettier --check --plugin-search-dir=."
        ]
    },
    "prisma": {
        "seed": "node --experimental-specifier-resolution=node --loader ts-node/esm prisma/seed.ts"
    },
    "devDependencies": {
        "@amplitude/analytics-browser": "^1.13.4",
        "@commitlint/cli": "^17.8.1",
        "@commitlint/config-conventional": "^17.8.1",
        "@faker-js/faker": "^7.6.0",
        "@floating-ui/dom": "^1.5.3",
        "@fortawesome/free-brands-svg-icons": "^6.5.1",
        "@fortawesome/free-regular-svg-icons": "^6.5.1",
        "@fortawesome/free-solid-svg-icons": "^6.5.1",
        "@planetscale/database": "^1.13.0",
        "@playwright/test": "^1.40.1",
        "@prisma/client": "^4.16.2",
        "@skeletonlabs/skeleton": "^2.6.1",
        "@skeletonlabs/tw-plugin": "^0.3.0",
        "@sveltejs/adapter-auto": "^2.1.1",
        "@sveltejs/adapter-vercel": "^2.4.3",
        "@sveltejs/kit": "^1.30.3",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/svelte": "^4.0.5",
        "@types/bcryptjs": "^2.4.6",
        "@types/node": "^18.19.3",
        "@types/testing-library__jest-dom": "^5.14.9",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "@vercel/node": "^2.15.10",
        "@vitest/coverage-v8": "^1.1.0",
        "autoprefixer": "^10.4.16",
        "bcryptjs": "^2.4.3",
        "concurrently": "^8.2.2",
        "dotenv": "^16.3.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-svelte": "^2.35.1",
        "husky": "^8.0.3",
        "jsdom": "^21.1.2",
        "lint-staged": "^13.3.0",
        "module-alias": "^2.2.3",
        "postcss": "^8.4.33",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "prisma": "^4.16.2",
        "svelte": "^3.59.2",
        "svelte-check": "^3.6.2",
        "svelte-fa": "^3.0.4",
        "svelte-forms-lib": "^2.0.1",
        "svelte-input-mask": "^1.1.0",
        "tailwindcss": "^3.4.0",
        "ts-interface-checker": "^1.0.2",
        "ts-node": "^10.9.2",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3",
        "uuid": "^9.0.1",
        "vercel": "^30.2.3",
        "vite": "^4.5.1",
        "vite-plugin-tailwind-purgecss": "^0.2.0",
        "vitest": "^1.1.0",
        "yup": "^1.3.3"
    },
    "type": "module"
}
