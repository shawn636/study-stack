{
	"name": "equipped",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lintfix": "prettier --plugin-search-dir . --write . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": "eslint",
		"*.{js,ts,svelte,css,scss,postcss,md,json}": [
			"prettier --write --plugin-search-dir=.",
			"prettier --check --plugin-search-dir=."
		]
	},
	"devDependencies": {
		"@amplitude/analytics-browser": "^1.9.3",
		"@commitlint/cli": "^17.5.0",
		"@commitlint/config-conventional": "^17.4.4",
		"@playwright/test": "^1.32.1",
		"@prisma/client": "^4.12.0",
		"@supabase/auth-helpers-sveltekit": "^0.9.0",
		"@supabase/supabase-js": "^2.12.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-vercel": "^2.4.1",
		"@sveltejs/kit": "^1.14.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"@vercel/node": "^2.10.1",
		"@vitest/coverage-c8": "^0.29.7",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"jsdom": "^21.1.1",
		"lint-staged": "^13.2.0",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"prisma": "^4.12.0",
		"svelte": "^3.57.0",
		"svelte-check": "^3.1.4",
		"svelte-heros-v2": "^0.4.2",
		"svelte-preprocess": "^5.0.3",
		"svelte-use-click-outside": "^1.0.0",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "^5.0.2",
		"vercel": "^28.16.12",
		"vite": "^4.1.4",
		"vitest": "^0.29.2"
	},
	"type": "module"
}
