{
    "name": "equipped",
    "version": "0.3.0",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "pscale:branch:create": "scripts/pscale/create-branch.sh",
        "pscale:branch:delete": "scripts/pscale/delete-branch.sh",
        "pscale:branch:delete-all-dev": "scripts/pscale/delete-all-dev-branches.sh",
        "pscale:cred:delete-all": "scripts/pscale/delete-all-creds.sh",
        "pscale:cred:delete-all-ci": "scripts/pscale/delete-all-ci-creds.sh",
        "pscale:cred:create": "scripts/pscale/create-cred.sh",
        "pscale:dr:create": "scripts/pscale/create-deploy-request.sh",
        "pscale:dr:close": "scripts/pscale/close-deploy-request.sh",
        "pscale:dr:apply": "scripts/pscale/apply-deploy-request.sh",
        "postinstall": "prisma generate",
        "vercel:deploy:preview": "scripts/vercel/deploy-preview.sh",
        "vercel:settings:pull": "scripts/vercel/pull-settings.sh",
        "_vercel-start": "vercel dev --scope equipped-team --token $VERCEL_TOKEN --yes --listen $PORT",
        "vercel-dev": "dotenv -v PORT=3000 -- pnpm _vercel-start",
        "vercel-unit-test": "dotenv -v PORT=3004 -- pnpm _vercel-start",
        "vercel-e2e-test": "dotenv -v PORT=3005 -- pnpm _vercel-start",
        "build": "vite build",
        "build:production": "vite build --mode production",
        "preview": "vite preview",
        "test": "playwright install && playwright test",
        "check": "velte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "concurrently \"pnpm run vercel-unit-test\" \"vitest --run\" --kill-others -s first",
        "test:watch": "concurrently \"pnpm run vercel-unit-test\" \"vitest\" --kill-others",
        "codegen": "concurrently \"pnpm run vercel-e2e-test\" \"playwright codegen\" --kill-others",
        "test:ui": "concurrently \"pnpm run vercel-e2e-test\" \"playwright test --ui\" --kill-others",
        "test:coverage": "concurrently \"pnpm run vercel-unit-test\" \"vitest run --coverage\" --kill-others -s first",
        "test:results": "pnpm exec playwright show-report e2e/test-results",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "lintfix": "prettier --plugin-search-dir . --write . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.{js,ts,svelte}": "eslint",
        "*.{js,ts,svelte,css,scss,postcss,md,json}": [
            "prettier --write --plugin-search-dir=.",
            "prettier --check --plugin-search-dir=."
        ]
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "devDependencies": {
        "@amplitude/analytics-browser": "^2.6.1",
        "@commitlint/cli": "^19.2.1",
        "@commitlint/config-conventional": "^19.1.0",
        "@faker-js/faker": "^8.4.1",
        "@floating-ui/dom": "^1.6.3",
        "@fortawesome/free-brands-svg-icons": "^6.5.1",
        "@fortawesome/free-regular-svg-icons": "^6.5.1",
        "@fortawesome/free-solid-svg-icons": "^6.5.1",
        "@paralleldrive/cuid2": "^2.2.2",
        "@planetscale/database": "^1.16.0",
        "@playwright/test": "^1.42.1",
        "@prisma/adapter-planetscale": "^5.11.0",
        "@prisma/client": "^5.11.0",
        "@skeletonlabs/skeleton": "^2.9.0",
        "@skeletonlabs/tw-plugin": "^0.3.1",
        "@sveltejs/adapter-auto": "^3.2.0",
        "@sveltejs/adapter-vercel": "^5.2.0",
        "@sveltejs/kit": "^2.5.5",
        "@sveltejs/vite-plugin-svelte": "^3.0.2",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.12",
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/svelte": "^4.1.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/node": "^20.12.2",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.4.0",
        "@typescript-eslint/parser": "^7.4.0",
        "@vercel/analytics": "^1.2.2",
        "@vercel/node": "^3.0.25",
        "@vercel/speed-insights": "^1.0.10",
        "@vitest/coverage-v8": "^1.4.0",
        "autoprefixer": "^10.4.19",
        "bcryptjs": "^2.4.3",
        "concurrently": "^8.2.2",
        "dotenv": "^16.4.5",
        "dotenv-cli": "^7.4.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-filename-rules": "^1.3.1",
        "eslint-plugin-perfectionist": "^2.7.0",
        "eslint-plugin-svelte": "^2.35.1",
        "husky": "^9.0.11",
        "jsdom": "^24.0.0",
        "kysely": "^0.27.3",
        "kysely-planetscale": "^1.4.0",
        "lint-staged": "^15.2.2",
        "module-alias": "^2.2.3",
        "postcss": "^8.4.38",
        "prettier": "^3.2.5",
        "prettier-eslint": "^16.3.0",
        "prettier-plugin-sh": "^0.14.0",
        "prettier-plugin-svelte": "^3.2.2",
        "prettier-plugin-tailwindcss": "^0.5.13",
        "prisma": "^5.11.0",
        "prisma-kysely": "^1.8.0",
        "svelte": "^4.2.12",
        "svelte-check": "^3.6.8",
        "svelte-fa": "^4.0.2",
        "svelte-forms-lib": "^2.0.1",
        "svelte-input-mask": "^1.1.0",
        "svelte-migrate": "^1.3.8",
        "tailwindcss": "^3.4.3",
        "ts-interface-checker": "^1.0.2",
        "ts-node": "^10.9.2",
        "tslib": "^2.6.2",
        "tsx": "^4.7.1",
        "typescript": "^5.4.3",
        "uuid": "^9.0.1",
        "vercel": "^33.6.2",
        "vite": "^5.2.7",
        "vite-plugin-tailwind-purgecss": "^0.2.0",
        "vitest": "^1.4.0",
        "yup": "^1.4.0"
    },
    "type": "module",
    "dependencies": {
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.1.3"
    },
    "packageManager": "pnpm@8.15.4+sha256.cea6d0bdf2de3a0549582da3983c70c92ffc577ff4410cbf190817ddc35137c2"
}
