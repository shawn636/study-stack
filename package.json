{
    "name": "equipped",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "vercel-dev": "vercel dev --token $VERCEL_TOKEN --yes",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "concurrently \"npm run vercel-dev\" \"vitest --run\" --kill-others -s first",
        "test:watch": "concurrently \"npm run vercel-dev\" \"vitest\" --kill-others",
        "codegen": "concurrently \"npm run vercel-dev\" \"playwright codegen\" --kill-others",
        "test:ui": "concurrently \"npm run vercel-dev\" \"playwright test --ui\" --kill-others",
        "test:coverage": "concurrently \"npm run vercel-dev\" \"vitest run --coverage\" --kill-others -s first",
        "test:results": "npx playwright show-report e2e/test-results",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "lintfix": "prettier --plugin-search-dir . --write . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*.{js,ts,svelte}": "eslint",
        "*.{js,ts,svelte,css,scss,postcss,md,json}": [
            "prettier --write --plugin-search-dir=.",
            "prettier --check --plugin-search-dir=."
        ]
    },
    "prisma": {
        "seed": "node --experimental-specifier-resolution=node --loader ts-node/esm prisma/seed.ts"
    },
    "devDependencies": {
        "@amplitude/analytics-browser": "^1.11.0",
        "@commitlint/cli": "^17.6.5",
        "@commitlint/config-conventional": "^17.6.5",
        "@faker-js/faker": "^7.6.0",
        "@floating-ui/dom": "^1.3.0",
        "@fortawesome/free-brands-svg-icons": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@planetscale/database": "^1.7.0",
        "@playwright/test": "^1.35.0",
        "@prisma/client": "^4.15.0",
        "@skeletonlabs/skeleton": "^1.7.1",
        "@sveltejs/adapter-auto": "^2.1.0",
        "@sveltejs/adapter-vercel": "^2.4.3",
        "@sveltejs/kit": "^1.20.2",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.9",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/svelte": "^3.2.2",
        "@types/bcryptjs": "^2.4.2",
        "@types/node": "^18.16.18",
        "@types/testing-library__jest-dom": "^5.14.6",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "@vercel/node": "^2.15.1",
        "@vitest/coverage-c8": "^0.29.8",
        "@vitest/coverage-istanbul": "^0.30.1",
        "autoprefixer": "^10.4.14",
        "bcryptjs": "^2.4.3",
        "concurrently": "^8.2.0",
        "dotenv": "^16.1.4",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-svelte": "^2.30.0",
        "husky": "^8.0.3",
        "jsdom": "^21.1.2",
        "lint-staged": "^13.2.2",
        "module-alias": "^2.2.3",
        "postcss": "^8.4.24",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "prisma": "^4.15.0",
        "svelte": "^3.59.1",
        "svelte-check": "^3.4.3",
        "svelte-fa": "^3.0.4",
        "svelte-forms-lib": "^2.0.1",
        "tailwindcss": "^3.3.2",
        "ts-interface-checker": "^1.0.2",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.3",
        "typescript": "^5.1.3",
        "uuid": "^9.0.0",
        "vercel": "^30.2.1",
        "vite": "^4.3.9",
        "vitest": "^0.30.1",
        "yup": "^1.2.0"
    },
    "type": "module"
}
